 {
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "Onboard BIG-IP",
    "Common": {
        "class": "Tenant",
        "mySystem": {
                "class": "System",
                "hostname": "bigip.azure.8f2e.com"
            },
    "myDns": {
        "class": "DNS",
        "nameServers": [
                "168.63.129.16"
        ],
        "search": [
            "f5.com"
        ]
    },
    "myNtp": {
        "class": "NTP",
        "servers": [
                "0.pool.ntp.org","1.pool.ntp.org","2.pool.ntp.org"
        ],
        "timezone": "UTC"
    },
    "external-public-subnet": {
            "class": "VLAN",
    "tag": 4093,
    "mtu": 1500,
    "interfaces": [
        {
            "name": "1.1",
            "tagged": false
        }
    ],
    "cmpHash": "dst-ip"
},
"external-public-subnet-self": {
    "class": "SelfIp",
    "address": "10.2.2.6/24",
    "vlan": "external-public-subnet",
    "allowService": ["tcp:1028"],
    "trafficGroup": "traffic-group-local-only"
 },
   "default": {
      "class": "Route",
      "gw": "10.2.2.1",
      "network": "default",
      "mtu": 1500
     }
  }
}

