{
  "class": "Telemetry",
  "My_System": {
      "class": "Telemetry_System",
      "systemPoller": {
          "interval": 60
      }
  },
  "My_Listener": {
      "class": "Telemetry_Listener",
      "port": 6514
  },
  "My_ELK_Consumer": {
    "class": "Telemetry_Consumer",
    "type": "Generic_HTTP",
    "host": "${logStashIP}",
    "protocol": "http",
    "port": ${logStashPort},
    "path": "/",
    "method": "POST",
    "headers": [
         {
           "name": "content-type",
           "value": "application/json"
         }
     ]
 }
}