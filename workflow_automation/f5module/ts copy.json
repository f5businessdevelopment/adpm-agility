{
  "class": "Telemetry",
  "controls": {
    "class": "Controls",
    "logLevel": "debug"
  },
  "My_Poller": {
    "class": "Telemetry_System_Poller",
    "interval": 60
  },
  "My_Listener": {
    "class": "Telemetry_Listener",
    "port": 6514
  },
  "My_Azure_Consumer": {
     "class": "Telemetry_Consumer",
     "type": "Azure_Log_Analytics",
     "workspaceId": "${law_id}",
     "passphrase": {
         "cipherText": "${law_primarykey}"
     },
     "useManagedIdentity": false
   }
}